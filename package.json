{
  "name": "rom-toolkit",
  "version": "1.5.0",
  "private": true,
  "dependencies": {
    "@fontsource/roboto": "^4.5.8",
    "@fontsource/roboto-mono": "^4.5.8",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.10",
    "@types/file-saver": "^2.0.5",
    "@types/lodash": "^4.14.190",
    "@types/md5": "^2.3.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "buffer": "^6.0.3",
    "comlink": "^4.3.1",
    "crc": "^4.2.0",
    "file-saver": "^2.0.5",
    "jssha": "^3.3.0",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "rommage": "^3.1.2",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint-staged": "lint-staged",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    },
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "homepage": "https://kinetikeith.github.io/rom-toolkit/",
  "repository": {
    "type": "git",
    "url": "https://github.com/kinetikeith/rom-toolkit"
  }
}
